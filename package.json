{
  "name": "node-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node dist",
    "serve": "ts-node src/index.ts --require ts-node/register",
    "build": "./node_modules/typescript/bin/tsc",
    "test": "NODE_ENV=test nyc mocha --opts ./test/main.opts --require ts-node/register",
    "test:unit": "NODE_ENV=test nyc mocha --opts ./test/mocha.opts",
    "test:integration": "NODE_ENV=test nyc mocha --opts ./test/mocha.integration.opts --require ts-node/register",
    "coverage": "NODE_ENV=test nyc report --reporter=text-lcov | coveralls",
    "seed:db": "sequelize db:seed:all --env=production",
    "clear:db": "sequelize db:seed:undo:all --env=production"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".es6"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
